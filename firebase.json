{
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run lint",
      "npm --prefix \"$RESOURCE_DIR\" run build"
    ],
    "source": "functions"
  },
  "hosting": [
    {
      "target": "dtps",
      "rewrites": [
        {
          "source": "**",
          "function": "dtps"
        }
      ]
    }
  ],
  "emulators": {
    "functions": {
      "port": 8083
    },
    "firestore": {
      "port": 8084
    },
    "ui": {
      "enabled": true,
      "port": 8085
    }
  }
}
